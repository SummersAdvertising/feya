
<header>
  <h2>加盟資訊</h2>
</header>
<article>
  <div class="list">
    <table width="100%" border="0" cellspacing="0" cellpadding="0">
    <% @tickets.each do | ticket | %>	    
      <tr>
        <td width="85%"><%= link_to ticket.name, admin_ticket_path( ticket ) %></td>
        <td width="15%" align="center"><%= ticket.created_at.strftime('%Y-%m-%d') %></td>
        <td><a href="<%= admin_ticket_path( ticket ) %>" data-method="delete" data-confirm="確定刪除此篇詢問？"><img src="/images/trash.png"></a></td>
      </tr>
    <% end %>
    </table>
  </div>
  <div class="page"><%= paginate @tickets %></div>
</article>